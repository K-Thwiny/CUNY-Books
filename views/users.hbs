<div class="container">
    <h3>List of Users</h3> 
    <div>
        {{# each users }}
            <article class="item">
                <div>Name: {{ this.fullName }}</div>
                <div>Email: {{ this.email }}</div> 
                <div>College: {{ this.college}}</div>
            </article>
        {{/each}}
    </div>
</div>